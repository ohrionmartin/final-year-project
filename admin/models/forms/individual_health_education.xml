<?xml version="1.0" encoding="utf-8"?>
<form
	addrulepath="/administrator/components/com_ehealth_portal/models/rules"
	addfieldpath="/administrator/components/com_ehealth_portal/models/fields"
>
	<fieldset name="details">
		<!-- [Fields 636] Default Fields. -->
		<!-- [Fields 638] Id Field. Type: Text (joomla) -->
		<field
			name="id"
			type="text" class="readonly" label="JGLOBAL_FIELD_ID_LABEL"
			description ="JGLOBAL_FIELD_ID_DESC" size="10" default="0"
			readonly="true"
		/>
		<!-- [Fields 657] Date Created Field. Type: Calendar (joomla) -->
		<field
			name="created"
			type="calendar"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_CREATED_DATE_LABEL"
			description="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_CREATED_DATE_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
		<!-- [Fields 680] User Created Field. Type: User (joomla) -->
		<field
			name="created_by"
			type="user"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_CREATED_BY_LABEL"
			description="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_CREATED_BY_DESC"
		/>
		<!-- [Fields 700] Published Field. Type: List (joomla) -->
		<field name="published" type="list" label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC" class="chzn-color-state"
			filter="intval" size="1" default="1" >
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="2">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>
		</field>
		<!-- [Fields 727] Date Modified Field. Type: Calendar (joomla) -->
		<field name="modified" type="calendar" class="readonly"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_MODIFIED_DATE_LABEL" description="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_MODIFIED_DATE_DESC"
			size="22" readonly="true" format="%Y-%m-%d %H:%M:%S" filter="user_utc" />
		<!-- [Fields 742] User Modified Field. Type: User (joomla) -->
		<field name="modified_by" type="user"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_MODIFIED_BY_LABEL"
			description="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_MODIFIED_BY_DESC"
			class="readonly"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 764] Access Field. Type: Accesslevel (joomla) -->
		<field name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC"
			default="1"
			required="false"
		/>
		<!-- [Fields 783] Ordering Field. Type: Numbers (joomla) -->
		<field
			name="ordering"
			type="number"
			class="inputbox validate-ordering"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_ORDERING_LABEL"
			description=""
			default="0"
			size="6"
			required="false"
		/>
		<!-- [Fields 806] Version Field. Type: Text (joomla) -->
		<field
			name="version"
			type="text"
			class="readonly"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_VERSION_LABEL"
			description="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_VERSION_DESC"
			size="6"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 869] Dynamic Fields. -->
		<!-- [Fields 2335] Individual_health_edu Field. Type: Individualhealthedu. (custom) -->
		<field
			type="individualhealthedu"
			name="individual_health_edu"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_INDIVIDUAL_HEALTH_EDU_LABEL"
			description="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_INDIVIDUAL_HEALTH_EDU_DESCRIPTION"
			class="list_class"
			multiple="false"
			default="0"
			button="true"
		/>
		<!-- [Fields 1937] Patient Field. Type: User. (joomla) -->
		<field
			type="user"
			name="patient"
			label="COM_EHEALTH_PORTAL_INDIVIDUAL_HEALTH_EDUCATION_PATIENT_LABEL"
		/>
	</fieldset>
	
	<!-- [Interpretation 19419] Access Control Fields. -->
	<fieldset name="accesscontrol">
		<!-- [Interpretation 19424] Asset Id Field. Type: Hidden (joomla) -->
		<field
			name="asset_id"
			type="hidden"
			filter="unset"
		/>
		<!-- [Interpretation 19432] Rules Field. Type: Rules (joomla) -->
		<field
			name="rules"
			type="rules"
			label="Permissions in relation to this individual_health_education"
			translate_label="false"
			filter="rules"
			validate="rules"
			class="inputbox"
			component="com_ehealth_portal"
			section="individual_health_education"
		/>
	</fieldset>
</form>