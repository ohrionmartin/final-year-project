<?xml version="1.0" encoding="utf-8"?>
<form
	addrulepath="/administrator/components/com_ehealth_portal/models/rules"
	addfieldpath="/administrator/components/com_ehealth_portal/models/fields"
>
	<fieldset name="details">
		<!-- [Fields 636] Default Fields. -->
		<!-- [Fields 638] Id Field. Type: Text (joomla) -->
		<field
			name="id"
			type="text" class="readonly" label="JGLOBAL_FIELD_ID_LABEL"
			description ="JGLOBAL_FIELD_ID_DESC" size="10" default="0"
			readonly="true"
		/>
		<!-- [Fields 657] Date Created Field. Type: Calendar (joomla) -->
		<field
			name="created"
			type="calendar"
			label="COM_EHEALTH_PORTAL_VMMC_CREATED_DATE_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_CREATED_DATE_DESC"
			size="22"
			format="%Y-%m-%d %H:%M:%S"
			filter="user_utc"
		/>
		<!-- [Fields 680] User Created Field. Type: User (joomla) -->
		<field
			name="created_by"
			type="user"
			label="COM_EHEALTH_PORTAL_VMMC_CREATED_BY_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_CREATED_BY_DESC"
		/>
		<!-- [Fields 700] Published Field. Type: List (joomla) -->
		<field name="published" type="list" label="JSTATUS"
			description="JFIELD_PUBLISHED_DESC" class="chzn-color-state"
			filter="intval" size="1" default="1" >
			<option value="1">
				JPUBLISHED</option>
			<option value="0">
				JUNPUBLISHED</option>
			<option value="2">
				JARCHIVED</option>
			<option value="-2">
				JTRASHED</option>
		</field>
		<!-- [Fields 727] Date Modified Field. Type: Calendar (joomla) -->
		<field name="modified" type="calendar" class="readonly"
			label="COM_EHEALTH_PORTAL_VMMC_MODIFIED_DATE_LABEL" description="COM_EHEALTH_PORTAL_VMMC_MODIFIED_DATE_DESC"
			size="22" readonly="true" format="%Y-%m-%d %H:%M:%S" filter="user_utc" />
		<!-- [Fields 742] User Modified Field. Type: User (joomla) -->
		<field name="modified_by" type="user"
			label="COM_EHEALTH_PORTAL_VMMC_MODIFIED_BY_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_MODIFIED_BY_DESC"
			class="readonly"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 764] Access Field. Type: Accesslevel (joomla) -->
		<field name="access"
			type="accesslevel"
			label="JFIELD_ACCESS_LABEL"
			description="JFIELD_ACCESS_DESC"
			default="1"
			required="false"
		/>
		<!-- [Fields 783] Ordering Field. Type: Numbers (joomla) -->
		<field
			name="ordering"
			type="number"
			class="inputbox validate-ordering"
			label="COM_EHEALTH_PORTAL_VMMC_ORDERING_LABEL"
			description=""
			default="0"
			size="6"
			required="false"
		/>
		<!-- [Fields 806] Version Field. Type: Text (joomla) -->
		<field
			name="version"
			type="text"
			class="readonly"
			label="COM_EHEALTH_PORTAL_VMMC_VERSION_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_VERSION_DESC"
			size="6"
			readonly="true"
			filter="unset"
		/>
		<!-- [Fields 869] Dynamic Fields. -->
		<!-- [Fields 1650] Are_you_circumcised Field. Type: List. (joomla) -->
		<field
			type="list"
			name="are_you_circumcised"
			label="COM_EHEALTH_PORTAL_VMMC_ARE_YOU_CIRCUMCISED_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_ARE_YOU_CIRCUMCISED_DESCRIPTION"
			class="list_class"
			multiple="false"
			required="true"
			useglobal="false"
			default="0">
			<!-- [Fields 1913] Option Set. -->
			<option value="0">
				COM_EHEALTH_PORTAL_VMMC_YES</option>
			<option value="1">
				COM_EHEALTH_PORTAL_VMMC_NO</option>
		</field>
		<!-- [Fields 1937] Patient Field. Type: User. (joomla) -->
		<field
			type="user"
			name="patient"
			label="COM_EHEALTH_PORTAL_VMMC_PATIENT_LABEL"
		/>
		<!-- [Fields 1650] Info_ben_vmcc Field. Type: List. (joomla) -->
		<field
			type="list"
			name="info_ben_vmcc"
			label="COM_EHEALTH_PORTAL_VMMC_INFO_BEN_VMCC_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_INFO_BEN_VMCC_DESCRIPTION"
			class="list_class"
			multiple="false"
			required="true"
			useglobal="false"
			default="0">
			<!-- [Fields 1913] Option Set. -->
			<option value="0">
				COM_EHEALTH_PORTAL_VMMC_YES</option>
			<option value="1">
				COM_EHEALTH_PORTAL_VMMC_NO</option>
		</field>
		<!-- [Fields 1650] Interested_in_vmmc Field. Type: List. (joomla) -->
		<field
			type="list"
			name="interested_in_vmmc"
			label="COM_EHEALTH_PORTAL_VMMC_INTERESTED_IN_VMMC_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_INTERESTED_IN_VMMC_DESCRIPTION"
			class="list_class"
			multiple="false"
			required="true"
			useglobal="false"
			default="0">
			<!-- [Fields 1913] Option Set. -->
			<option value="0">
				COM_EHEALTH_PORTAL_VMMC_YES</option>
			<option value="1">
				COM_EHEALTH_PORTAL_VMMC_NO</option>
		</field>
		<!-- [Fields 1650] Vmmc_gender Field. Type: List. (joomla) -->
		<field
			type="list"
			name="vmmc_gender"
			label="COM_EHEALTH_PORTAL_VMMC_VMMC_GENDER_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_VMMC_GENDER_DESCRIPTION"
			class="list_class"
			multiple="false"
			required="true"
			useglobal="false"
			default="0">
			<!-- [Fields 1913] Option Set. -->
			<option value="0">
				COM_EHEALTH_PORTAL_VMMC_MALE</option>
			<option value="1">
				COM_EHEALTH_PORTAL_VMMC_FEMALE</option>
		</field>
		<!-- [Fields 1650] Partner_circumcised Field. Type: List. (joomla) -->
		<field
			type="list"
			name="partner_circumcised"
			label="COM_EHEALTH_PORTAL_VMMC_PARTNER_CIRCUMCISED_LABEL"
			description="COM_EHEALTH_PORTAL_VMMC_PARTNER_CIRCUMCISED_DESCRIPTION"
			class="list_class"
			multiple="false"
			required="true"
			useglobal="false"
			default="0">
			<!-- [Fields 1913] Option Set. -->
			<option value="0">
				COM_EHEALTH_PORTAL_VMMC_YES</option>
			<option value="1">
				COM_EHEALTH_PORTAL_VMMC_NO</option>
		</field>
	</fieldset>
	
	<!-- [Interpretation 19419] Access Control Fields. -->
	<fieldset name="accesscontrol">
		<!-- [Interpretation 19424] Asset Id Field. Type: Hidden (joomla) -->
		<field
			name="asset_id"
			type="hidden"
			filter="unset"
		/>
		<!-- [Interpretation 19432] Rules Field. Type: Rules (joomla) -->
		<field
			name="rules"
			type="rules"
			label="Permissions in relation to this vmmc"
			translate_label="false"
			filter="rules"
			validate="rules"
			class="inputbox"
			component="com_ehealth_portal"
			section="vmmc"
		/>
	</fieldset>
</form>